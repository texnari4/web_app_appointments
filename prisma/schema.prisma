generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Master {
  id        String   @id @default(cuid())
  name      String
  phone     String?
  bio       String?   @db.Text
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
