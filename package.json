{
  "name": "web-app-appointments",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node --swc src/index.ts",
    "start": "npm run prisma:sync && node dist/index.js",
    "prisma:sync": "prisma migrate deploy && prisma db push && prisma generate",
    "seed": "ts-node --swc scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "prisma": "^5.22.0",
    "typescript": "^5.5.4",
    "ts-node": "^10.9.2",
    "@types/node": "^22.5.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5"
  }
}