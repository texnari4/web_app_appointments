{
  "name": "web-app-appointments",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "npm run prisma:sync && node dist/index.js",
    "prisma:sync": "prisma migrate deploy && prisma db push && prisma generate",
    "seed": "node dist/scripts/seed.js",
    "dev": "ts-node --esm src/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.14.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}