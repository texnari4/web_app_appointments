{
  "name": "web-app-appointments",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prisma:sync": "prisma migrate deploy && prisma db push && prisma generate",
    "start": "npm run prisma:sync && node dist/index.js",
    "dev": "ts-node --esm src/index.ts"
  },
  "dependencies": {
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "@prisma/client": "^5.22.0",
    "typescript": "^5.6.3",
    "ts-node": "^10.9.2",
    "@types/node": "^22.7.5",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}