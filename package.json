    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "zod": "^3.23.8",
    "prisma": "^5.22.0",
    "typescript": "^5.6.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.16.11",
    "@types/cors": "^2.8.17"
  }
}