    "prisma:sync": "prisma migrate deploy && prisma generate",
    "seed": "node --loader ts-node/esm scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "cors": "2.8.5",
    "express": "5.1.0",
    "pino": "9.5.0",
    "pino-http": "10.3.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "prisma": "5.22.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "@types/node": "22.7.4",
    "@types/express": "4.17.21",
    "@types/cors": "2.8.17"
  }
}